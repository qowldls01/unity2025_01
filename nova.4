#include <stdio.h>
#include <stdlib.h>

typedef struct Node {
    int data;
    struct Node *next;
} Node;

void insert(Node **head, int val) {
    Node *newNode = malloc(sizeof(Node));
    newNode->data = val;
    newNode->next = *head;
    *head = newNode;
}

void delete(Node **head, int val) {
    Node *curr = *head, *prev = NULL;
    while (curr && curr->data != val) {
        prev = curr;
        curr = curr->next;
    }
    if (!curr) return;

    if (prev) prev->next = curr->next;
    else *head = curr->next;

    free(curr);
}

void print(Node *head) {
    while (head) {
        printf("%d -> ", head->data);
        head = head->next;
    }
    printf("NULL\n");
}

int main() {
    Node *head = NULL;

    insert(&head, 3);
    insert(&head, 5);
    insert(&head, 7);

    printf("리스트: ");
    print(head);

    delete(&head, 5);
    printf("삭제 후: ");
    print(head);

    return 0;
}
